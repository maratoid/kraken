ansible_python_interpreter: "PATH=/home/core/bin:$PATH python"
master_record: <%= access_scheme %>://<%= master_ip %>:<%= access_port %>
proxy_record: <%= node_001_ip %>
dns_ip: <%= dns_ip %>
