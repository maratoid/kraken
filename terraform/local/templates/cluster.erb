kubernetes_api_version: <%= kubernetes_api_version %>
kubernetes_binaries_uri: <%= kubernetes_binaries_uri %>
logentries_token: <%= logentries_token %>
logentries_url: <%= logentries_url %>
apiserver_nginx_pool: <%= apiserver_nginx_pool %>
